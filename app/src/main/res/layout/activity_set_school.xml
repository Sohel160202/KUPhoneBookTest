<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SetSchool"
    android:background="@drawable/background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"></include>

        <ImageView
            android:id="@+id/HeadlineBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            app:srcCompat="@drawable/headline" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="92dp"
            android:layout_marginRight="92dp"
            android:text="SET"
            android:textColor="@color/colorPrimaryDark"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/HeadlineBox"
            app:layout_constraintTop_toTopOf="@+id/HeadlineBox" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintEnd_toStartOf="@+id/textView"
            app:layout_constraintStart_toStartOf="@+id/HeadlineBox"
            app:layout_constraintTop_toTopOf="@+id/HeadlineBox"
            app:srcCompat="@drawable/buildingicon" />

        <ImageView
            android:id="@+id/ArchiBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/HeadlineBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/Archi"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="@+id/ArchiBox"
            app:layout_constraintEnd_toEndOf="@+id/ArchiBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/ArchiBox"
            app:layout_constraintTop_toTopOf="@+id/ArchiBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/archi_text" />

        <ImageView
            android:id="@+id/Archi_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintEnd_toStartOf="@+id/ArchiBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/HeadlineBox"
            app:srcCompat="@drawable/archi_icon" />

        <ImageView
            android:id="@+id/CSEBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ArchiBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/CSE"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/CSEBox"
            app:layout_constraintEnd_toEndOf="@+id/CSEBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/CSEBox"
            app:layout_constraintTop_toTopOf="@+id/CSEBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/cse_text" />

        <ImageView
            android:id="@+id/Cse_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toStartOf="@+id/CSEBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Archi"
            app:srcCompat="@drawable/cse_icon" />

        <ImageView
            android:id="@+id/URPBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CSEBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/URP"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:contentDescription="TODO"
            app:layout_constraintBottom_toBottomOf="@+id/URPBox"
            app:layout_constraintEnd_toEndOf="@+id/URPBox"
            app:layout_constraintStart_toStartOf="@+id/URPBox"
            app:layout_constraintTop_toTopOf="@+id/URPBox"
            app:srcCompat="@drawable/urp_text" />

        <ImageView
            android:id="@+id/Urp_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="23dp"
            app:layout_constraintEnd_toStartOf="@+id/URPBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/CSE"
            app:srcCompat="@drawable/urp_icon" />

        <ImageView
            android:id="@+id/EceBox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/URPBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/ECE"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/EceBox"
            app:layout_constraintEnd_toEndOf="@+id/EceBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/EceBox"
            app:layout_constraintTop_toTopOf="@+id/EceBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/ece_text" />

        <ImageView
            android:id="@+id/Ece_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toStartOf="@+id/EceBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/URP"
            app:srcCompat="@drawable/ece_icon" />

        <ImageView
            android:id="@+id/MathBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/EceBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/Math"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/MathBox"
            app:layout_constraintEnd_toEndOf="@+id/MathBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/MathBox"
            app:layout_constraintTop_toTopOf="@+id/MathBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/math_text" />

        <ImageView
            android:id="@+id/Math_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toStartOf="@+id/MathBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ECE"
            app:srcCompat="@drawable/math_icon" />

        <ImageView
            android:id="@+id/PhysicsBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/MathBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/Physics"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/PhysicsBox"
            app:layout_constraintEnd_toEndOf="@+id/PhysicsBox"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/PhysicsBox"
            app:layout_constraintTop_toTopOf="@+id/PhysicsBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/physics_text" />

        <ImageView
            android:id="@+id/Physics_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toStartOf="@+id/PhysicsBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Math"
            app:srcCompat="@drawable/physics_icon" />

        <ImageView
            android:id="@+id/ChemistryBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/PhysicsBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/Chemistry"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/ChemistryBox"
            app:layout_constraintEnd_toEndOf="@+id/ChemistryBox"
            app:layout_constraintHorizontal_bias="0.471"
            app:layout_constraintStart_toStartOf="@+id/ChemistryBox"
            app:layout_constraintTop_toTopOf="@+id/ChemistryBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/chemistry_text" />

        <ImageView
            android:id="@+id/Chemistry_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="23dp"
            app:layout_constraintEnd_toStartOf="@+id/ChemistryBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Physics"
            app:srcCompat="@drawable/chemistry_icon" />

        <ImageView
            android:id="@+id/StatBox"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ChemistryBox"
            app:srcCompat="@drawable/departmentbox" />

        <ImageView
            android:id="@+id/Stat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/StatBox"
            app:layout_constraintEnd_toEndOf="@+id/StatBox"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintStart_toStartOf="@+id/StatBox"
            app:layout_constraintTop_toTopOf="@+id/StatBox"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/stat_text" />

        <ImageView
            android:id="@+id/Stat_Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            app:layout_constraintEnd_toStartOf="@+id/StatBox"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Chemistry"
            app:srcCompat="@drawable/stat_icon" />





    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>